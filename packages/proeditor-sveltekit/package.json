{
  "name": "@shelchin/proeditor-sveltekit",
  "version": "0.1.0",
  "description": "High-performance text/CSV editor for SvelteKit with virtual scrolling and 10M+ rows support",
  "type": "module",
  "svelte": "./src/index.ts",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "svelte": "./src/index.ts",
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./core": {
      "types": "./src/core/index.ts",
      "default": "./src/core/index.ts"
    },
    "./render": {
      "types": "./src/render/index.ts",
      "default": "./src/render/index.ts"
    },
    "./line-editor": {
      "svelte": "./src/line-editor/index.ts",
      "types": "./src/line-editor/index.ts",
      "default": "./src/line-editor/index.ts"
    },
    "./ui": {
      "svelte": "./src/ui/index.ts",
      "types": "./src/ui/index.ts",
      "default": "./src/ui/index.ts"
    },
    "./components": {
      "svelte": "./src/components/index.ts",
      "types": "./src/components/index.ts",
      "default": "./src/components/index.ts"
    },
    "./i18n": {
      "types": "./src/i18n/index.ts",
      "default": "./src/i18n/index.ts"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "@lucide/svelte": ">=0.400.0",
    "svelte": "^5.0.0"
  },
  "optionalDependencies": {
    "papaparse": "^5.4.0",
    "xlsx": "^0.18.0"
  },
  "devDependencies": {
    "@types/papaparse": "^5.3.14",
    "svelte-check": "^4.0.0",
    "typescript": "^5.5.0"
  },
  "keywords": [
    "svelte",
    "sveltekit",
    "editor",
    "csv",
    "text-editor",
    "virtual-scroll",
    "high-performance"
  ],
  "license": "MIT"
}
